{
  "name": "vue-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.20.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "run-p type-check build-only && cp dist/index.html dist/200.html",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --fix",
    "deploy:vercel": "npx vercel",
    "deploy:ntl": "npx ntl deploy",
    "deploy:surge": "npx surge dist"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "pinia": "^2.0.33",
    "postcss-nested": "^6.0.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@iconify-json/fluent-emoji": "^1.1.12",
    "@iconify-json/ph": "^1.1.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue-macros/volar": "^0.8.4",
    "eslint": "^8.35.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.29.0",
    "typescript": "~4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^1.9.1",
    "unplugin-vue-router": "^0.5.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
